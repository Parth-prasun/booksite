<% include partials/header %>
<div class="ui main text container segment">
    <div class="ui huge header">Edit <%= log.title %></div>
    <form class="ui form" action="/books/<%= log._id %>?_method=PUT" method="POST">
     <div class="field">
            <label>Name</label>
            <input type="text" name="name" value=" <%= log.contributor %>" >
        </div>
        <div class="field">
            <label>image</label>
            <input type="text" name="imageurl" value=" <%= log.image %>" >
        </div>
        <div class="field">
            <label>Phone Number</label>
            <input type="text" name="contact" value=" <%= log.phone %>" >
        </div>
        <div class="field">
            <label>Email</label>
            <input type="text" name="mail" value=" <%= log.email %>" >
        </div>
        <div class="field">
            <label>Books</label>
            <input id="book1" type="text" name="book1" value="<%=log.lists[0].title %>" >
            <input id="book2" type="text" name="book2" value="<%=log.lists[1].title %>" >
            <input id="book3" type="text" name="book3" value="<%=log.lists[2].title %>" >
            <input id="book4" type="text" name="book4" value="<%=log.lists[3].title %>" >
            <input id="book5" type="text" name="book5" value="<%=log.lists[4].title %>" >
            <input id="book6" type="text" name="book6" value="<%=log.lists[5].title %>" >
            <input id="book7" type="text" name="book7" value="<%=log.lists[6].title %>" >
            <input id="book8" type="text" name="book8" value="<%=log.lists[7].title %>" >
            <input id="book9" type="text" name="book9" value="<%=log.lists[8].title %>" >
            <input id="book10" type="text" name="book10" value"<%=log.lists[9].title %>" >
        </div>
        <div class="field">
            <label>Genre</label>
            <input type="text" name="gen" value=" <%= log.genre %>" >
        </div>
        <input class="ui violet big basic button" type="submit">

    </form>
</div>

<% include partials/footer %>